<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    targetNamespace="http://xml.netbeans.org/schema/books"
    xmlns:tns="http://xml.netbeans.org/schema/books"
    elementFormDefault="qualified" xmlns:ns1="http://onlinebookstore.com/schema/">
    <xsd:import schemaLocation="onlinebookstore.xsd" namespace="http://onlinebookstore.com/schema/"/>
    <xsd:element name="books">
        <xsd:complexType>
            <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="book" type="ns1:Book"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>